<h1>List of Users</h1>
<br>
<table>
  <thead>
    <tr>
    <th>Username </th>
    <th>Number of Articles</th>
    <th>Actions</th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user|%>
    <tr>
    <td><%= user.username%></td>
    <td><%= pluralize(user.articles.count,"article")%></td>
    <td><%=link_to 'edit profile', edit_user_path(user)%></td>
    <td><%=link_to 'user profile', user_path(user)%></td>
   </tr>
    <% end %>
  </tbody>
</table>  