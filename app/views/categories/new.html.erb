<h1>Create New Category .</h1>

<%= render 'form' %>
<%= form_for :category ,url: categories_path do |f| %>
    <p>
    <%= f.label :name %> <br/>
    <%= f.text_field :name %>
    </p>

    <p>
    <%= f.submit %>
    </p>
    <% end %>

    <p><%= link_to 'Return to List of categories' ,categories_path %></p>
