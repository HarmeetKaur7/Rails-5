<h1>Hi From edit page</h1>

<%= render 'form' %>

<%= form_for :article ,url: article_path(@article) ,method: :patch do |f| %>
    <p>
    <%= f.label :title %> <br/>
    <%= f.text_field :title %>
    </p>
    <p>
    <%= f.label :description %> <br/>
    <%= f.text_area :description%>
    </p>
    <p>
    <%= f.submit %>
    </p>
    <% end %>

    <p> <%= link_to 'Cancel Edit' ,articles_path %></p>